{
  "name": "node-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "build": "rm -rf ./src/dist && babel ./src/. --out-dir ./dist --copy-files",
    "migrate": "sequelize db:migrate --env production",
    "migrate-dev": "sequelize db:migrate",
    "start": "npm run build && node ./dist/index.js",
    "seed": "sequelize db:seed:all --env production",
    "seed-dev": "sequelize db:seed:all --env development",
    "dev": "yarn migrate-dev && yarn seed-dev && ./node_modules/.bin/nodemon --exec babel-node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "createModel": "npx sequelize-cli model:generate ",
    "createSeed": "npx sequelize-cli seed:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drayzii/node-setup.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drayzii/node-setup/issues"
  },
  "homepage": "https://github.com/drayzii/node-setup#readme",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@hapi/joi": "^16.1.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.6",
    "sequelize-cli": "^5.5.1",
    "swagger-jsdoc": "3.0.2",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "prettier": "^2.3.2"
  }
}
